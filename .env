# -------- APP -------- #
DEBUG=true

# -------- NATS -------- #
NATS_DEBUG=true
NATS_SERVER=nats://127.0.0.1:4222
# Max number of pings the client will allow unanswered before raising a stale connection error
NATS_PASS=
# Number of milliseconds between client-sent pings
NATS_PING_INTERVAL=120000
# If false server will not attempt reconnecting
NATS_RECONNECT=true
# If disconnected, the client will wait the specified number of milliseconds between reconnect attempts
NATS_RECONNECT_TIME_WAIT=2000
# Max number of pings the client will allow unanswered before raising a stale connection error
NATS_MAX_PING_OUT=-1
# Number of milliseconds the client will wait for a connection to be established. The value of 0 specifies no limit
NATS_TIMEOUT=0
# Sets the maximum number of reconnect attempts. The value of -1 specifies no limit
NATS_MAX_RECONNECT_ATTEMPTS=-1
# Default queue
NATS_DEFAULT_QUEUE=true
# Queue prefix
NATS_QUEUE_PREFIX=

# -------- Ð¡LIENT -------- #
GUI_HOST=http://localhost:8080